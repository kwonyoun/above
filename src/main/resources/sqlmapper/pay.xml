<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.above.dao.PayDao">

    <insert id="paySuccess" >
        insert into pay values(default, ${paid_amount}, #{imp_uid} )
    </insert>

    <select id="payInfoSelect" parameterType="String" resultType="com.example.above.vo.PayinfoVO" >
        select * from pay where imp_uid = #{imp_uid}
    </select>

</mapper>